  "$id": "https://loopstack.ai/schemas/action.schema.json",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "service": {
      "type": "string"
    },
    "inputs": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "output": {
      "type": "string"
    },
    "props": {
      "type": "object"
    }
  },
